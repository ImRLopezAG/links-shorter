{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "baseUrl": ".",
    "paths": {
      "@db/*": [
        "src/db/*"
      ],
      "@/*": [
        "src/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
    }
  }
}